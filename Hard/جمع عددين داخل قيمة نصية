
function addStrNums(num1, num2) {
    // write your code here
    if (isNaN(num1) || isNaN(num2)) {
        let result = -1
        return result.toString();
    } else if (!isNaN(num1) && !isNaN(num2)){
        let result =  +num1 + +num2
        return result.toString()
    
    }
}

console.log(addStrNums("10","m"))



